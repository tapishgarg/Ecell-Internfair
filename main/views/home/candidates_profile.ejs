<body>

<div class="page-loading">
    <img src="/jobhunt/images/loader.gif" alt=""/>
    <!-- 	<span>Skip Loader</span>
     --></div>

<div class="theme-layout" id="scrollup">
    <!-- MENU -->
    <% include ../common/navigationbar.ejs %>

    <section class="overlape">
        <div class="block no-padding">
            <div data-velocity="-.1" style="background-image: url(jobhunt/images/home4.jpeg)" repeat scroll 50% 422.28px transparent;" class="parallax scrolly-invisible no-parallax"></div><!-- PARALLAX BACKGROUND IMAGE -->
            <div class="container fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="inner-header">
                            <h3>Welcome ALI TUFAN</h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section>
        <div class="block no-padding">
            <div class="container">
                 <div class="row no-gape">
                    <aside class="col-lg-3 column border-right">

                        <% include ../home/candidates_dashboard.ejs %>
                        
                        
                    </aside>
                    <div class="col-lg-9 column">
                        <div class="padding-left">
                            <div class="profile-form-edit">
                                <form>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <span class="pf-title">Full Name</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="Ali TUFAN" />
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <span class="pf-title">Roll No.</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="ME15B036" />
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <span class="pf-title">College</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="IITM" />
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <span class="pf-title">Branch</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="Mechanical Engineering" />
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <span class="pf-title">CGPA</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="Mechanical Engineering" />
                                            </div>
                                        </div>

                                        
                                    </div>
                                </form>
                            </div>
                            <div class="contact-edit">
                                <h3>Contact</h3>
                                <form>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <span class="pf-title">Contact Number</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="Ali TUFAN" />
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <span class="pf-title">Alternate Number</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="ME15B036" />
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <span class="pf-title">Email</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="IITM" />
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <span class="pf-title">Alternate Email</span>
                                            <div class="pf-field">
                                                <input type="text" placeholder="Mechanical Engineering" />
                                            </div>
                                        </div>
                                        
                                        <div class="col-lg-12">
                                            <button type="submit">Update</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                 </div>
            </div>
        </div>
    </section>

    <% include ../common/footer.ejs %>

</div>

<% include ../common/login.ejs %>

<% include ../common/signup.ejs %>

